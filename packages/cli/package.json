{
  "name": "figtree",
  "description": "env files shouldn't exist.",
  "version": "0.0.0",
  "author": "maxchehab @maxchehab",
  "bin": {
    "figtree": "./bin/run"
  },
  "bugs": "https://github.com/maxchehab/figtree/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/maxchehab/figtree",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "figtree",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "maxchehab/figtree",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-help": "2.2.1",
    "@types/node": "12.11.7",
    "axios": "0.19.0",
    "chalk": "2.4.2",
    "globby": "10.0.1",
    "open": "7.0.0",
    "ts-node": "8.4.1",
    "tslib": "1.10.0",
    "typescript": "3.6.4",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/axios": "0.14.0",
    "@types/chalk": "2.2.0",
    "@types/node": "12.11.7",
    "@types/open": "6.2.1",
    "globby": "10.0.1",
    "ts-node": "8.4.1",
    "typescript": "3.6.4"
  }
}
